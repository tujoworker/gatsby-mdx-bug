{
  "private": true,
  "name": "gatsby-mdx-bug",
  "description": "gatsby-mdx-bug",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "yarn make-ui-lib-pages",
    "build-ci": "yarn build-portal:version && yarn make-ui-lib-pages && yarn build-portal --prefix-paths && yarn commit-new-pages",
    "build-portal": "gatsby build",
    "build-portal:fresh": "rm -rf .cache && rm -rf ./public/** && yarn build-portal",
    "build-portal:stage": "yarn build-portal --prefix-paths",
    "build-portal:version": "babel-node ./scripts/version.js --new-version",
    "precommit": "yarn lint-staged",
    "commit-new-pages": "babel-node ./scripts/commitPages.js",
    "deploy": "yarn deploy-ci",
    "deploy-ci": "babel-node ./scripts/deploy.js",
    "deploy:fast": "yarn build-portal --prefix-paths && babel-node ./scripts/deploy.js",
    "dev": "nodemon --exec 'gatsby develop' -w 'gatsby-node.js' -w 'gatsby-config.js'",
    "lint": "eslint --quiet ./src",
    "lint-ci": "yarn lint:js && yarn lint:styles",
    "lint-staged": "lint-staged",
    "lint:js": "yarn lint --fix",
    "lint:styles": "stylelint './src/**/*.{js,css}'",
    "make-ui-lib-pages": "babel-node ./scripts/uilib/makeDemosFactory.js && yarn prettier:components",
    "make-ui-lib-pages:dev": "nodemon --exec 'yarn make-ui-lib-pages' -e js -w '**/makeDemosFactory.js' -w '**/tasks/*' -w '**/Template.js'  -w '**/template.md'",
    "reset": "cd ../../ && yarn reset && cd - && yarn build",
    "serve": "live-server public --watch --no-browser --port=8001",
    "start": "cross-env DEBUG=gatsby:query-watcher gatsby develop",
    "test": "jest --watch",
    "test-ci": "jest --ci",
    "test:staged": "jest --bail --findRelatedTests :#keep this whitespace",
    "test:update": "jest --updateSnapshot",
    "prettier:components": "prettier --loglevel warn --write './src/uilib/{components,patterns}/**/*.{js,md}' './src/pages/uilib/{components,elements,patterns}/*.{js,md}'",
    "prettier:diff": "prettier --list-different '{scripts,stories,src}/**/*.{js,md}'",
    "prettier:js": "prettier --loglevel warn --write '{scripts,stories,src}/**/*.js'",
    "prettier:other": "prettier --loglevel warn --write 'src/**/*.{json,md}' --write '.babelrc' && yarn prettier:package",
    "prettier:package": "prettier-package-json --write ./package.json"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "prism-react-renderer": "^0.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@mdx-js/mdx": "0.16.0",
    "@mdx-js/tag": "0.16.1",
    "autoprefixer": "^9.4.2",
    "babel-plugin-emotion": "^9.2.11",
    "camelcase": "^5.0.0",
    "ci-info": "^2.0.0",
    "classnames": "^2.2.6",
    "clean-html": "^1.5.0",
    "color": "^3.1.0",
    "cross-env": "^5.2.0",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "eslint": "^5.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "front-matter": "^3.0.1",
    "gatsby": "^2.0.67",
    "gatsby-link": "^2.0.7",
    "gatsby-mdx": "^0.2.0",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-emotion": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-postcss": "^2.0.2",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.12",
    "gh-pages": "^2.0.1",
    "live-server": "^1.2.1",
    "nodemon": "^1.18.8",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^8.0.9",
    "postcss-preset-env": "^6.5.0",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "react-live": "^1.12.0"
  },
  "buildVersion": "[LOCAL BUILD]"
}
